{
	"name": "project-versioning",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"migrate": "node scripts/migration.mjs",
		"migrate:fresh": "node scripts/migration.mjs --fresh",
		"seed": "node scripts/seeder.mjs",
		"test": "vitest run"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.87.3",
		"dotenv": "^17.2.3"
	},
	"engines": {
		"node": ">=20 <26"
	},
	"pnpm": {
		"overrides": {
			"cookie": "^0.7.0",
			"tar": "^7.5.4"
		}
	}
}